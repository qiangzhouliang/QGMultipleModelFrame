<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--自定义linearlayout可以换行显示-->
    <declare-styleable name="AutoLineFeedLinearLayout">
        <!--每一行显示多少个-->
        <attr name="auto_line_num" format="integer"/>
        <!--行与行之间的间距-->
        <attr name="auto_line_top" format="dimension"/>
    </declare-styleable>
</resources>